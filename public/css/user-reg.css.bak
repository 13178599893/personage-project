body{
	text-align:center;
	background-image: url("../img/reg-bg.jpg");
}
div.div-locating{
	margin:0 auto;
}
.ico{
	position: relative;
	top:30px;
	width:30px;
}
a .ico{
	position: relative;
	top:0px;
	width:30px;
}
.ico.ico-phone{
	width:25px;
	margin-right:5px;
}
.div-border{
	border:1px solid white;
	background-color:#a4c3be;
	width:35%;
/*	height:550px;*/
	position: absolute;
	left:30%;
	top:10%;
	border-radius:10%;
	box-shadow:2px 9px 25px #000;
}
button{
	width:400px;
	height:40px;
	background-color:#a4c3be;
	position: relative;
	top:20px;
	left:15px;
	margin-top:20px;
	border-radius:16px ;
}
.div-box{
	position: relative;
	top:15px;
	right:15px;
	margin-top:20px;
	margin-bottom:20px;	
	font-family:"宋体";
}
.input{
	position: relative;
	top:20px;
	margin-top:20px;
	width:400px;
	height:45px;
	border-radius:16px;
}
ul{
	list-style-type:none;
}
.div-end{
	position: relative;
	top:50px;
	font-family:"宋体";
}
.div-first{
	position: relative;
	top:10px;
	font-style: italic;
	font-size:30px;
}
hr{
	position: relative;
	top:40px;
	margin-top:20px;
}
.div-endend{
	position: relative;
	top:100px;
	font-family:"宋体";
	font-size:10px;
	margin-bottom:30px;
}
.div-logo img{
	text-align:center;
	width:250px;
}
.div-elselogin{
	position: relative;
	top:50px;
	font-family:"宋体";
	font-size:12px;
	margin-left:20px;
	margin-right:20px;
}
.div-elselogin a img{
	width:20px;
	margin-left:10px;
	margin-right:10px;
}
.div-box-reg{
	position: relative;
	top:20px;
	right:25px;
	margin-top:10px;
	margin-right:-20px;
	font-family:"宋体";
}
.div-box-reg a{
	margin-left:10px;
	position: relative;
	left:60px;
}
.div-hr{
	position: relative;
	top:-15px;
}